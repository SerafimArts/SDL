{
    "name": "serafim/ffi-sdl",
    "description": "PHP FFI to SDL2 bindings",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/SerafimArts/SDL/issues",
        "source": "https://github.com/SerafimArts/SDL"
    },
    "keywords": [
    ],
    "homepage": "https://github.com/SerafimArts/SDL",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*",
        "composer-plugin-api": "^1.1",
        "composer/composer": "~1.9",
        "serafim/ffi-utils": "dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\SDL\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "symfony/var-dumper": "~5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\SDL\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "class": "Serafim\\SDL\\Library\\Compiler",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
